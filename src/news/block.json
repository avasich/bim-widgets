{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "bim-widgets/bim-news",
  "title": "Bim News",
  "category": "widgets",
  "icon": "menu-alt2",
  "version": "0.1",
  "selectors": {
    "root": ".bim-news"
  },
  "attributes": {
    "news": {
      "type": "array",
      "default": [
        {
          "title": "",
          "url": "",
          "subtitle": ""
        }
      ]
    },
    "title": {
      "type": "string",
      "default": ""
    }
  },
  "supports": {
    "align": true
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "render": "file:./render.php"
}